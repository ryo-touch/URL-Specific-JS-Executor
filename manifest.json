{
  "manifest_version": 3,
  "name": "Execute JavaScript on Specific URLs",
  "author": "ryo-touch",
  "version": "1.1",
  "description": "Executes specific JavaScripts when specific URLs are visited.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.dropbox.com/*"],
      "js": ["dropbox_transfer.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
